{
    "name": "hs-util-ext-monorepo",
    "type": "module",
    "private": true,
    "workspaces": [
        "extension",
        "website",
        "api"
    ],
    "scripts": {
        "dev:ext": "cd extension && npm run dev",
        "dev:web": "cd website && npm run dev",
        "dev:api": "cd api && npm run dev",
        "build:ext": "cd extension && npm run build",
        "build:web": "cd website && npm run build",
        "build": "npm run build:ext"
    },
    "dependencies": {
        "vite": "^5.4.10"
    }
}
